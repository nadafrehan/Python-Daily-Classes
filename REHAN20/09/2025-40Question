        "  def __init__(self,page):\n",
        "    self.page = page\n",
        "  def __add__(self,other):\n",
        "    return self.page + other.page\n",
        "Obj1 = Book(200)\n",
        "obj2 = Book(300)\n",
        "print(\"TOtal page is\",Obj1 + obj2)"
