        "from abc import ABC, abstractmethod\n",
        "class Vehicle(ABC):\n",
        "    @abstractmethod\n",
        "    def sound(self):\n",
        "        pass\n",
        "\n",
        "class Car(Vehicle):\n",
        "    def sound(self):\n",
        "        print(\"I am driving a car\")\n",
        "\n",
        "obj = Car()\n",
        "obj.sound()\n"
      ],
