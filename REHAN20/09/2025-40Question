        "# Quiz Competition\n",
        "def ask_question(question, options, answer):\n",
        "    print(\"\\n\" + question)\n",
        "    for key, value in options.items():\n",
        "        print(f\"{key}. {value}\")\n",
        "\n",
        "    user_choice = input(\"Enter your choice (a/b/c/d): \").lower()\n",
        "\n",
        "    match user_choice:\n",
        "        case 'a' | 'b' | 'c' | 'd':\n",
        "            if user_choice == answer:\n",
        "                print(\"âœ… Correct Answer!\")\n",
        "                return 1\n",
        "            else:\n",
        "                print(\"âœ–ï¸ Wrong Answer!\")\n",
        "                return 0\n",
        "        case _:\n",
        "            print(\"âœ–ï¸ Invalid Choice\")\n",
        "            return 0\n",
        "\n",
        "print(\"Welcome to Python Quiz Competition\")\n",
        "player_name = input(\"Enter your name: \")\n",
        "score = 0\n",
        "\n",
        "questions = [\n",
        "    {\"q\": \"1) Which of the following is a valid variable name?\",\n",
        "     \"options\": {\"a\": \"1var\", \"b\": \"var1\", \"c\": \"var-1\", \"d\": \"var 1\"}, \"ans\": \"b\"},\n",
        "\n",
